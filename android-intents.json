{
  "android_app_links": {
    "package_name": "net.sigmaaudley.app",
    "app_name": "Sigma Audley Official", 
    "deep_link_patterns": [
      {
        "pattern": "https://sigmaaudley.net/",
        "activity": "MainActivity",
        "description": "Homepage - Main landing page"
      },
      {
        "pattern": "https://sigmaaudley.net/products/*",
        "activity": "ProductActivity",
        "description": "Individual product pages"
      },
      {
        "pattern": "https://sigmaaudley.net/complete-research-peptide-catalog.html",
        "activity": "CatalogActivity", 
        "description": "Complete product catalog"
      },
      {
        "pattern": "https://sigmaaudley.net/glp1-forum-verified-excellence.html",
        "activity": "TrustActivity",
        "description": "GLP1Forum verification page"
      },
      {
        "pattern": "https://sigmaaudley.net/reddit-peptides-trusted-supplier.html", 
        "activity": "TrustActivity",
        "description": "Reddit verification page"
      },
      {
        "pattern": "https://sigmaaudley.net/certificate-of-analysis.html",
        "activity": "QualityActivity",
        "description": "Quality certificates page"
      }
    ],
    "intent_filters": [
      {
        "action": "android.intent.action.VIEW",
        "category": ["android.intent.category.DEFAULT", "android.intent.category.BROWSABLE"],
        "data": {
          "scheme": "https",
          "host": "sigmaaudley.net"
        }
      },
      {
        "action": "android.intent.action.SEARCH", 
        "category": ["android.intent.category.DEFAULT"],
        "data": {
          "mimeType": "vnd.android.cursor.item/vnd.sigmaaudley.peptide"
        }
      }
    ]
  },
  "custom_url_schemes": {
